{
  "name": "vite-react-ts-templete",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "preview": "vite preview",
